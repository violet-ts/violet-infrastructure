{
  "name": "violet-infrastructure",
  "version": "1.0.0",
  "private": true,
  "license": "TODO",
  "scripts": {
    "compile": "pnpm run --filter=./pkg/shared compile && pnpm run --filter=./pkg --filter=./pkg/shared",
    "cdktf-prepare:manager": "run-p cdktf-prepare:_:*",
    "cdktf-prepare:env": "pnpm run cdktf-prepare:_:compile-bot",
    "cdktf-prepare:_:compile-bot": "pnpm run --filter=./pkg/bot compile",
    "cdktf-prepare:_:compile-portal": "pnpm run --filter=./pkg/portal/api compile",
    "cdktf-prepare:_:zip-self": "ts-node -T ./pkg/shared/scripts/local/zip-self.ts",
    "lint": "eslint \"**/*.{js,cjs,mjs,ts,tsx,json}\"",
    "lint-fix": "pnpm run lint -- --fix",
    "lint-quiet": "pnpm run lint -- --quiet",
    "test": "pnpm run --filter=./pkg test",
    "test-watch": "pnpm run --filter=./pkg test-watch",
    "dev:portal": "pnpm run --filter=./pkg/portal dev"
  },
  "devDependencies": {
    "@cdktf/provider-aws": "^4.0.15",
    "@cdktf/provider-null": "^0.4.156",
    "@cdktf/provider-random": "^0.0.20",
    "@portal/api": "workspace:*",
    "@portal/web": "workspace:*",
    "@self/bot": "workspace:*",
    "@self/def-env": "workspace:*",
    "@self/def-manager": "workspace:*",
    "@self/shared": "workspace:*",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/promise-queue": "^2.2.0",
    "arg": "^5.0.1",
    "cdktf": "0.8.6",
    "cdktf-cli": "^0.8.6",
    "constructs": "^10.0.41",
    "dotenv": "^14.2.0",
    "esbuild": "^0.14.13",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^7.32.0",
    "fast-glob": "^3.2.11",
    "jest": "^27.4.7",
    "node-dev": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "promise-queue": "^2.2.5",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "wait-on": "^6.0.0",
    "zod": "^3.11.6"
  },
  "engines": {
    "node": ">=14",
    "npm": "use pnpm",
    "pnpm": ">=6",
    "yarn": "use pnpm"
  }
}
